module DifferenceOfSums = struct

    type t = { x: int; x2: int }
    
    val private add_t : int * t -> t
    let add_t i t =
        let { t; ~x; ~x2 } = t in
        { t with x = x + i; x2 = x2 + i*i }

    val main : unit -> unit
    let main () =
        let sum_t = Loops.for_i 1 101 add_t { x = 0; x2 = 0 } in
        let { sum_t; ~x; ~x2 } = sum_t in
        let () = Print.int (x*x - x2); Print.newline () in
        free sum_t        
        
end
